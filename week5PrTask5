#include "pch.h"
#include <iostream>
using namespace std;

int main()
{
	int number;
	cin >> number;

	if(number >=10 && number<=200)
	{
		for (int i = number; i > 0; i--)
		{
			if (i % 7 == 0)
			{
				cout << i << endl;

			}
		}
	}
	else
	{
		cout << "Error.Input a new value!" << endl;
	}
	
	return 0;
}
